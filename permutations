#include <cs50.h>
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int base = 5;
int counter = 0;


int main(void) {
    
    for (int a = 1; a <= base; a++) // column 1
    {
        for (int b = 1; b <= base; b++) // column 2
        {
            
            if (b != a)
            {
                for (int c = 1; c <= base; c++) // column 3
                {
                    
                    if ( (c != a) && (c != b) )
                    {
                        for (int d = 1; d <= base; d++) // column 4
                        {
                            
                            if ( (d != a) && (d != b) && (d != c) )
                            {
                                for (int e = 1; e <= base; e++) // column 5
                                {
                                    
                                    if ( (e != a) && (e != b) && (e != c) && (e != d) ) {
                                        printf("%i, %i, %i, %i, %i\n", a, b, c, d, e);
                                        counter++;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    
    printf("permutations: %i\n", counter);
    
}
